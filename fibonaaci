

//fibonacci without recursion
import java.util.*;
class fibonacci
{
	public static void main(String[] args) 
	{	Scanner scan=new Scanner(System.in);
		int next;
		System.out.println("Enter till how many numbers of fibonacci you want to know");
		int no=scan.nextInt();
		int first=0;
		int second=1;
System.out.println(first);
System.out.println(second);
		for(int i=0;i<no;i++)
			{next=first+second;
				System.out.println(next);
				
				first=second;
				second=next;
			}
	}
}
